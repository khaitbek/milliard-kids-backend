{
  "name": "million-kids-backend",
  "version": "0.0.0",
  "description": "Express.js framework boilerplate with Typescript and Prisma",
  "main": "app.js",
  "scripts": {
    "tsc": "tsc",
    "start": "node dist/app.js",
    "dev": "tsup ./src/app.ts --watch --onSuccess 'node dist/app.js'",
    "build": "tsup ./src/.",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "express"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "author": "Hayitbek Yusupov",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "express": "^4.17.3",
    "express-validator": "^7.0.1",
    "husky": "^7.0.4",
    "jsonwebtoken": "^9.0.1",
    "lint-staged": "^12.3.7",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "passport": "^0.5.2",
    "prettier": "^2.6.0",
    "prisma": "^4.16.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.7.0",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.6.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/passport": "^1.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^7.1.0",
    "typescript": "^5.2.0-dev.20230629"
  }
}
